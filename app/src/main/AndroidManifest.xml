            android:theme="@style/AppTheme.NoActionBar." />
        <activity android:name=".ConnectBin" android:screenOrientation="portrait" />
        <activity android:name=".Facts" />
        <activity android:name=".UseBin" android:screenOrientation="portrait" />
        <activity
            android:name=".IntroVideo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
