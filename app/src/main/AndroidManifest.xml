        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m" >
        <activity android:name=".adminHome" >
        </activity>
        <activity android:name=".PointsRedeem" />
        <activity android:name=".MoreInformation" />
        <activity android:name=".Help" />
        <activity android:name=".TandC2" />
        <activity android:name=".PointsDetails" />
        <activity android:name=".MyProfile" />
        <activity android:name=".UserTransactions" />
        <activity
